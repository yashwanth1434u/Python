#pattern program
n=int(input())
for i in range(1,n+1):
    for j in range(1,i+1):
        print('*', end=' ')
    print() 


#pattern2
n=int(input())
for i in range(1,n+1):
    for j in range(1,i+1):
        print(i, end=' ')
    print() 



#pattern3
n=int(input())
for i in range(n,0,-1):
    for j in range(1,i+1):
        print(j, end=' ')
    print()
