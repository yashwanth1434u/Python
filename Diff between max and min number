n=int(input('Enter no of elements you want to enter'))
n1=int(input('Enter elements'))
large=n1
small=n1
i=1
while(i<n):
    n1=int(input('Enter elements'))
    if(n1>large):
        large=n1
    elif n1<small:
        small=n1
    i=i+1
print('largest',large) print('smallest',small)
differ=large-small
print('Difference:',differ)
